{
  "name": "stripe-server",
  "version": "1.0.0",
  "engines": {
    "node": "v14.16.1",
    "npm": "6.14.12"
  },
  "scripts": {
    "start": "node server.js",
    "build": "cd client/ && npm install && npm run build",
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "firebase": "^8.4.2",
    "react-router-redux": "^4.0.8",
    "stripe": "^8.145.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
